<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BookDetailActivity">

    <TextView
        android:id="@+id/tv_bookdetail_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="title"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.524"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.229" />

    <TextView
        android:id="@+id/tv_bookdetail_bookid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="172dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="180dp"
        android:layout_marginBottom="510dp"
        android:text="bookid"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.535"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_bookdetail_author"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv_bookdetail_author"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="author"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.528"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_bookdetail_title"
        app:layout_constraintVertical_bias="0.095" />

    <TextView
        android:id="@+id/tv_bookdetail_isbn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="isbn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_bookdetail_bookid"
        app:layout_constraintVertical_bias="0.231" />

    <TextView
        android:id="@+id/tv_bookdetail_titlelable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/book_detail_title_lable"
        android:textColor="#040000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_bookdetail_title"
        app:layout_constraintHorizontal_bias="0.477"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.202" />

    <TextView
        android:id="@+id/tv_bookdetail_authorlable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/book_detail_author_lable"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_bookdetail_author"
        app:layout_constraintHorizontal_bias="0.549"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_bookdetail_titlelable"
        app:layout_constraintVertical_bias="0.053" />

    <TextView
        android:id="@+id/tv_bookdetail_bookidlable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/book_detail_bookid_lable"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_bookdetail_bookid"
        app:layout_constraintHorizontal_bias="0.528"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_bookdetail_authorlable"
        app:layout_constraintVertical_bias="0.054" />

    <TextView
        android:id="@+id/tv_bookdetail_isbn_lable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/book_detail_isbn_lable"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_bookdetail_isbn"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_bookdetail_bookidlable"
        app:layout_constraintVertical_bias="0.125" />

    <Button
        android:id="@+id/btn_bookdetail_makeappoint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btn_add_appointment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.249"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.848" />
</androidx.constraintlayout.widget.ConstraintLayout>